{
  "name": "auth",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "cross-env NODE_ENV=development nest start --watch ts",
    "start:debug": "nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=production node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "pretest:e2e": "docker-compose up -d test-db",
    "posttest:e2e": "docker-compose stop test-db && docker-compose rm -f test-db",
    "generate:fixtures": "fixtures ./src/fixtures --config ./ormconfig.js",
    "migration:run": "npx typeorm migration:run",
    "migration:revert": "npx typeorm migration:revert"
  },
  "dependencies": {
    "@azure/ms-rest-js": "^2.0.4",
